{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": ["./src/*"],
        }
    },
    "include": ["src/**/*", "index.ts"],
    "exclude": ["node_modules", "dist"]
}
